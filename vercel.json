{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/google2e9d921d285b1a0a.html",
            "dest": "/public/google2e9d921d285b1a0a.html"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}